{
  "deltaCount": {
    "background-color": {
      "value": "$semantic.color.surface.default",
      "type": "color"
    },
    "border-color": {
      "value": "$semantic.color.border.neutral.default",
      "type": "color"
    },
    "border-width": {
      "value": "$semantic.border-width.default",
      "type": "borderWidth"
    },
    "counterRemove": {
      "text-color": {
        "value": "$semantic.color.text.danger",
        "type": "color"
      },
      "icon-color": {
        "value": "$semantic.color.icon.danger",
        "type": "color"
      }
    },
    "counterChange": {
      "text-color": {
        "value": "$semantic.color.text.warning",
        "type": "color"
      },
      "icon-color": {
        "value": "$semantic.color.icon.warning",
        "type": "color"
      }
    },
    "font": {
      "value": "{typography.n.h6}",
      "type": "typography"
    }
  }
}