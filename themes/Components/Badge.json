{
  "badge": {
    "primary": {
      "text-color": {
        "value": "$color.text.onSolid",
        "type": "color"
      },
      "icon-color": {
        "value": "$color.icon.onSolid",
        "type": "color"
      },
      "success": {
        "default": {
          "background-color": {
            "value": "$color.status.success.default",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.success.weak",
            "type": "color"
          }
        }
      },
      "info": {
        "default": {
          "background-color": {
            "value": "$color.status.info.default",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.info.weak",
            "type": "color"
          }
        }
      },
      "warning": {
        "default": {
          "background-color": {
            "value": "$color.status.warning.default",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.warning.weak",
            "type": "color"
          }
        }
      },
      "critical": {
        "default": {
          "background-color": {
            "value": "$color.status.critical.default",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.critical.weak",
            "type": "color"
          }
        }
      },
      "neutral": {
        "default": {
          "background-color": {
            "value": "$color.status.neutral.default",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.neutral.weak",
            "type": "color"
          }
        }
      }
    },
    "secondary": {
      "text-color": {
        "value": "$color.text.onSolidInversed",
        "type": "color"
      },
      "icon-color": {
        "value": "$color.icon.onSolidInversed",
        "type": "color"
      },
      "success": {
        "default": {
          "background-color": {
            "value": "$color.status.success.weakest",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.success.weaker",
            "type": "color"
          }
        }
      },
      "info": {
        "default": {
          "background-color": {
            "value": "$color.status.info.weakest",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.info.weaker",
            "type": "color"
          }
        }
      },
      "warning": {
        "default": {
          "background-color": {
            "value": "$color.status.warning.weakest",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.warning.weaker",
            "type": "color"
          }
        }
      },
      "critical": {
        "default": {
          "background-color": {
            "value": "$color.status.critical.weakest",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.critical.weaker",
            "type": "color"
          }
        }
      },
      "neutral": {
        "default": {
          "background-color": {
            "value": "$color.status.neutral.weakest",
            "type": "color"
          }
        },
        "hover": {
          "background-color": {
            "value": "$color.status.neutral.weaker",
            "type": "color"
          }
        }
      }
    },
    "pill": {
      "border-radius": {
        "value": "$borderRadius.16",
        "type": "borderRadius"
      }
    },
    "rectangle": {
      "border-radius": {
        "value": "$borderRadius.6",
        "type": "borderRadius"
      }
    },
    "font-family": {
      "value": "{nunitoSans}",
      "type": "fontFamilies"
    },
    "font-weight": {
      "value": "$fontWeight.Bold",
      "type": "fontWeights"
    },
    "font-size": {
      "value": "$fontSize.200",
      "type": "fontSizes"
    },
    "line-height": {
      "value": "$lineHeight.100",
      "type": "lineHeights"
    },
    "text-case": {
      "value": "$textCase.none",
      "type": "textCase"
    },
    "font": {
      "value": {
        "fontFamily": "{badge.font-family}",
        "fontWeight": "{badge.font-weight}",
        "lineHeight": "{badge.line-height}",
        "fontSize": "{badge.font-size}",
        "letterSpacing": "{letterSpacing.none}",
        "textDecoration": "{textDecoration.none}",
        "textCase": "{badge.text-case}"
      },
      "type": "typography"
    },
    "text-decoration": {
      "value": "$textDecoration.none",
      "type": "textDecoration"
    }
  }
}